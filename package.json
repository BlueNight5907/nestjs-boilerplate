{
  "name": "my-nestjs-boilerplate",
  "version": "10.0.0",
  "description": "My NestJS Boilerplate, Typescript, Postgres, TypeORM",
  "author": "BlueNight",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build:prod": "tsc -p tsconfig.build.json",
    "postbuild:prod": "copyfiles --up 1 src/**/*.json dist",
    "start:hmr": "node dist/main.hmr.ts",
    "start:dev": "ts-node -r main.ts",
    "start:prod": "NODE_PATH=dist node dist/main.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "yarn run typeorm migration:generate -d ormconfig.ts ./src/database/typeorm/migrations",
    "migration:create": "yarn run typeorm migration:create ./src/database/typeorm/migrations/$NAME",
    "migration:revert": "yarn run typeorm migration:revert -d ormconfig.ts",
    "schema:drop": "yarn run typeorm schema:drop",
    "watch:dev": "ts-node-dev -r tsconfig-paths/register src/main.ts",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' -r tsconfig-paths/register src/main.ts",
    "webpack": "webpack --config webpack.config.js --progress",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "release": "release-it"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.369.0",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^10.0.5",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.0",
    "bullmq": "^5.7.2",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-ctx": "^0.1.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nest-winston": "^1.9.4",
    "nestjs-i18n": "^10.4.0",
    "nestjs-typeorm-paginate": "^4.0.4",
    "node-cache": "^5.1.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.1",
    "qs": "^6.12.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^5.0.0",
    "tslib": "^2.6.2",
    "typeorm": "^0.3.17",
    "typeorm-transactional": "~0.4.1",
    "uuid": "^9.0.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.8",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mime-types": "^2.1.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-canonical": "^4.9.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.20.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "~13.2.3",
    "prettier": "^3.0.0",
    "release-it": "^16.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleDirectories": [
      "<rootDir>",
      "node_modules"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
